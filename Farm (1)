local function countTargetInventory()
    [cite_start]local total = 0 [cite: 6]
    [cite_start]local locations = {player.Backpack:GetChildren(), (player.Character and player.Character:GetChildren() or {})} [cite: 6]
    [cite_start]for _, list in pairs(locations) do [cite: 6]
        [cite_start]for _, item in pairs(list) do [cite: 6]
            [cite_start]if item:IsA("Tool") then [cite: 6]
                [cite_start]local bName = item:GetAttribute("BrainrotName") or item.Name [cite: 6]
                [cite_start]if bName == targetBrainrot and item:GetAttribute("Level") == targetLevel and item:GetAttribute("Mutation") == targetMutation then [cite: 6]
                    [cite_start]total = total + 1 [cite: 7]
                end
            end
        end
    end
    [cite_start]return total [cite: 7]
end

